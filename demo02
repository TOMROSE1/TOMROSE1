package day01;import java.util.Scanner;
public class demo08 {

	public static void main(String[] args) {
		//Math:数学，Math类中提供了常见数学函数
		//y=f(x)
		Scanner console=new Scanner(System.in);
		System.out.print("输入位移：");
		double s=console.nextDouble();
		double t;
		double g=9.8;
		double n=2*s/g;
		t=Math.sqrt(n);
		System.out.println("时间："+t);
	}

}
